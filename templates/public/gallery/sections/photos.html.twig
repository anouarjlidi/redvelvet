{% for photo in photos %}

    <div onclick="openModal(this)" class="gallery animated-element">

        <div class="photo">

            <img src="{{ asset('photos/'~photo.src) }}">

        </div>

    </div>

{% endfor %}

{% if photos | length == 0 %}

    <div class="no-data">

        <p>Atsiprašome, šiuo metu nuotraukų galerijoje nėra</p>

    </div>

{% endif %}